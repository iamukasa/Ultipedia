<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.oldultipedia"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.GALLERY" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.oldultipedia.UltiSplash"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.oldultipedia.QuizActivityMath" />
        <activity android:name="com.example.oldultipedia.UtilScores" />
        <activity android:name="com.example.oldultipedia.QuizActivityEng" />
        <activity android:name="com.example.oldultipedia.QuizActivityKis" />
        <activity android:name="com.example.oldultipedia.QuizActivitySci" />
        <activity android:name="com.example.oldultipedia.QuizActivitySocial" />
        <activity android:name="com.example.oldultipedia.QuizActivityCre" />
        <activity android:name="com.example.oldultipedia.UtilMenu" />
        <activity android:name="com.example.oldultipedia.QuizActivity" />
        <activity
            android:name="com.example.oldultipedia.Utilhelp"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" >
        </activity>
        <activity android:name="com.example.oldultipedia.UtilSettings" />
        <activity
            android:name="com.example.oldultipedia.UtilSubjectPicker"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name="com.example.oldultipedia.UtilMode"
            android:theme="@android:style/Theme.Dialog" />
    </application>

</manifest>